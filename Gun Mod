local library = loadstring(game:HttpGet("https://pastebin.com/raw/eKwyeQa0", true))()

local tab1 = library:CreateTab("Fast Gun Mod", true)

local button = library:MakeButton(tab1,"Get Gun",function(button)

local prevposition = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame

wait(0.1)

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(813, 100, 2249)

wait(0.3)

local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["M9"].ITEMPICKUP

local Event = game:GetService("Workspace").Remote.ItemHandler

Event:InvokeServer(A_1)

local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["Remington 870"].ITEMPICKUP

local Event = game:GetService("Workspace").Remote.ItemHandler

Event:InvokeServer(A_1)

wait(0)

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-938, 94, 2044)

wait(0.2)

local A_1 = game:GetService("Workspace")["Prison_ITEMS"].giver["AK-47"].ITEMPICKUP

local Event = game:GetService("Workspace").Remote.ItemHandler

Event:InvokeServer(A_1)

wait(0.3)

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = prevposition

end)

local button = library:MakeButton(tab1,"M9",function(button)

local player = game:GetService('Players').LocalPlayer

local gun = player.Backpack:FindFirstChild('M9')

local sM = require(gun:FindFirstChild('GunStates'))

sM ['Damage'] = 1

sM ['MaxAmmo'] = 15

sM ['StoredAmmo'] = 15

sM ['FireRate'] = math.huge

sM ['AmmoPerClip'] = 1

sM ['Range'] = math.huge

sM ['ReloadTime'] = math.huge

sM ['Bullets'] = 1

sM ['AutoFire'] = true

end)

local button = library:MakeButton(tab1,"AK-47",function(button)

local player = game:GetService('Players').LocalPlayer

local gun = player.Backpack:FindFirstChild('AK-47')

local sM = require(gun:FindFirstChild('GunStates'))

sM ['Damage'] = 1

sM ['MaxAmmo'] = 30

sM ['StoredAmmo'] = 30

sM ['FireRate'] = math.huge

sM ['AmmoPerClip'] = 1

sM ['Range'] = math.huge

sM ['ReloadTime'] = math.huge

sM ['Bullets'] = 1

sM ['AutoFire'] = true

end)

